icht das warten in sekunden statt microsekunden\ndef wait_sek(sekunden: number):\n    control.wait_micros(sekunden * 1000000)\n    print(\"warten beendet\")\nguesses = \"\"\nwait_sek(1)\n# Liste der Wörter erstellen, aus einem Bereich von 1 bis\n# Länge der word_list eine Zahl zufällig auswählen\nalphabet = [\"a\", \"b\", \"c\", \"d\", \"e\", \"f\", \"g\", \"h\", \"i\", \"j\", \"k\", \"l\", \"m\", \"n\", \"o\", \"p\", \"q\", \"r\", \"s\", \"t\", \"u\", \"v\", \"w\", \"x\", \"y\", \"z\"]\nword_list = [\"kacka\", \"alex\", \"sandra\", \"banane\", \"apfel\"]\nprint(len(word_list))\nprint(randint(1, len(word_list)))\n# debug zufällige zahl aus den wörtern\n# debug\nfor index in range(4):\n    word = word_list[randint(0, len(word_list) - 1)]\n    print(word)\n# bevor es weitergeht, KI anschalten experimentell :)\nword = word_list[randint(1, len(word_list) - 1)]\nprint(word)\n\n# determine the number of turns\nturns = 10\n\n#verzweifelter versuch buttonklick, zwekcs weiter ...\nbuttonklick = 0\nauswahl = 0\n\nprint(alphabet[0])\n#print(alphabet[auswahl])\nfor index in range(len(alphabet) - 1):\n            print(alphabet[index])\n\ndef auswahl_buchstabe():\n    len(alphabet) - 1\n    alphabet[0]\n    print(guess)\n    print(\"ist guess leer?\")\n    \n    while True:\n        if input.button_is_pressed(Button.A):\n            print(alphabet[auswahl])\n            auswahl = auswahl + 1\n        elif input.button_is_pressed(Button.B):\n            guess = (alphabet[auswahl])\n            return\n        #for index in range(len(alphabet) - 1):\n            #print(alphabet[index])\n\n#auswahl_buchstabe()\n\nwhile turns > 0:\n    # make a counter that starts with zero\n    failed = 0\n    # for every character in secret_word\n    for char in word:\n        # see if the character is in the players guess\n        if guesses.index_of(char) >= 0:\n            print(char)\n        else:\n            print(\"_\")\n            # and increase the failed counter with one\n            failed += 1\n    # if failed is equal to zero\n    # print You Won\n    if failed == 0:\n        print(\"You won\")\n        basic.show_string(\"You won\")\n        basic.show_icon(IconNames.HAPPY)\n        wait_sek(3)\n        # exit the script\n        break\n    \n    while buttonklick == 0:\n        if input.button_is_pressed(Button.A):\n            print(alphabet[auswahl])\n            auswahl = auswahl + 1\n            wait_sek(2)\n        elif input.button_is_pressed(Button.B):\n            guess = \"b\"\n            print(\"weiter gehts\")\n            buttonklick = 1\n    \n    buttonklick = 0 \n    #guess = \"x\"\n    print(guess)\n    # set the players guess to guesses\n    guesses = \"\" + guesses + guess\n    # if the guess is not found in the secret word\n    if word.index_of(guess) < 0:\n        # turns counter decreases with 1 (now 9)\n        turns += 0 - 1\n        # print wrong\n        basic.show_string(\"Wrong\")\n        basic.show_icon(IconNames.SAD)\n        wait_sek(3)\n        print(\"wrong\")\n        # how many turns are left\n        basic.show_number(turns)\n        wait_sek(3)\n        print(\"You have\")\n        print(turns)\n        print(\"more guesses\")\n        # if the turns are equal to zero\n        if turns == 0:\n            print(\"You Lose\")\n            basic.show_icon(IconNames.SKULL)\n            wait_sek(30000)\n\ndef on_forever():\n    pass\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"hangman\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}},{"timestamp":1756636559952,"editorVersion":"8.0.16","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\")RN8ZS),`bJ:n3Eg@le^\">word_list</variable><varia