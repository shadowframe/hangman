\n    \n    while buttonklick == 0:\n        if input.button_is_pressed(Button.A):\n            print(alphabet[auswahl])\n            auswahl = auswahl + 1\n            wait_sek(2)\n        elif input.button_is_pressed(Button.B):\n            guess = \"b\"\n            print(\"weiter gehts\")\n            buttonklick = 1\n    \n    buttonklick = 0 \n    #guess = \"x\"\n    print(guess)\n    # set the players guess to guesses\n    guesses = \"\" + guesses + guess\n    # if the guess is not found in the secret word\n    if word.index_of(guess) < 0:\n        # turns counter decreases with 1 (now 9)\n        turns += 0 - 1\n        # print wrong\n        basic.show_string(\"Wrong\")\n        basic.show_icon(IconNames.SAD)\n        wait_sek(3)\n        print(\"wrong\")\n        # how many turns are left\n        basic.show_number(turns)\n        wait_sek(3)\n        print(\"You have\")\n        print(turns)\n        print(\"more guesses\")\n        # if the turns are equal to zero\n        if turns == 0:\n            print(\"You Lose\")\n            basic.show_icon(IconNames.SKULL)\n            wait_sek(30000)\n\ndef on_forever():\n    pass\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"hangman\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}},{"timestamp":1756638634782,"editorVersion":"8.0.16","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\")RN8ZS),`bJ:n3Eg@le^\">word_list</variable><variable id=\"x_a2b+*)+*H+U0Fj1HCK\">word</variable><variable id=\"bkfcaSq7=Yr6@0_)CyTa\">turns</variable><variable id=\"m?zn5rWcBVh]c|dwuDh9\">failed</variable><variable id=\";c[LZ,G.$ZFeipB{[nmN\">char</variable><variable id=\".HQ.`DGnVd3E4/zQeh[=\">guesses</var