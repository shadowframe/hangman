tton.A, on_button_pressed_a)\n    \n    guess = \"x\"\n    print(guess)\n    # set the players guess to guesses\n    guesses = \"\" + guesses + guess\n    # if the guess is not found in the secret word\n    if word.index_of(guess) < 0:\n        # turns counter decreases with 1 (now 9)\n        turns += 0 - 1\n        # print wrong\n        basic.show_string(\"Wrong\")\n        basic.show_icon(IconNames.SAD)\n        wait_sek(3)\n        print(\"wrong\")\n        # how many turns are left\n        basic.show_number(turns)\n        wait_sek(3)\n        print(\"You have\")\n        print(turns)\n        print(\"more guesses\")\n        # if the turns are equal to zero\n        if turns == 0:\n            print(\"You Lose\")\n            basic.show_icon(IconNames.SKULL)\n            wait_sek(30000)\n\ndef on_forever():\n    pass\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"hangman\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}},{"timestamp":1756603577636,"editorVersion":"8.0.16","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\",/Z5`2[A1s#vpIOg_8vv\">word_list</variable><variable id=\"-tG?tr65r+}Jzlu($%)X\">word</variable><variable id=\"#odK56q2]MTz8rXP8S;l\">turns</variable><variable id=\"%2H$_MU*dp1BOy;y6HQ(\">buttonklick</variable><variable id=\"oKcee|pky,M+Rd5J%tW*\">failed</variable><variable id=\"?%;l=eETCk1_n|9+hT%P\">char</variable><variable id=\"*WVmG)3FWvxXs}wxOE,@\">guesses</variable><variable id=\"uHxN[K*[LY[?qHJ6hMow\">guess</variable></variables><block type=\"pxt-on-start\" x=\"20\" y=\"20\"><statement name=\"HANDLER\"><block type=\"typescript_statement\"><mutation xmlns=\"http://www.w3.org/1999/xhtml\" line0=\"let word: string